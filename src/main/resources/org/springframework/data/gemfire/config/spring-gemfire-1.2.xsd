            <xsd:attribute name="pool-name" type="xsd:string"
                use="optional">
                <xsd:annotation>
                    <xsd:documentation><![CDATA[
The name of the pool used by the index. Used usually in client scenarios.
					]]></xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
